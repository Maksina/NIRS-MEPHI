void BrushEvOdd(int length, double *x, double *ev, double *odd);

int main()
{	
    int length = 32;
    double x[32];
    double ev[15];
    double odd[15];
    int i;
    for (i = 0; i < lenght; i++){
        x[i] = rand()%10 +1;    
    }
    BrushEvOdd(length,x, ev, odd);
